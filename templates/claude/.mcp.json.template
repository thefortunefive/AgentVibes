{
  "name": "{{name}} Agent",
  "version": "1.0.0",
  "description": "{{description}}",
  "emoji": "{{emoji}}",
  "mcpServers": {
    "discord": {
      "command": "mcp-server-discord",
      "args": ["--channels", "team-{{teamNumber}}", "team-{{teamNumber}}-updates", "general"],
      "env": {
        "DISCORD_TOKEN": "${DISCORD_TOKEN}",
        "AGENT_NAME": "{{name}}",
        "AGENT_EMOJI": "{{emoji}}"
      }
    },
    "github": {
      "command": "mcp-server-github",
      "args": [],
      "env": {
        "GITHUB_TOKEN": "${GITHUB_TOKEN}",
        "AGENT_PREFIX": "{{emoji}} Team-{{teamNumber}}:"
      }
    },
    "browsertools": {
      "command": "mcp-server-browsertools",
      "args": [],
      "env": {
        "DEFAULT_URL": "http://localhost:{{ports.frontend}}"
      }
    },
    "puppeteer": {
      "command": "mcp-server-puppeteer",
      "args": [],
      "env": {
        "DEFAULT_VIEWPORT": "1920x1080"
      }
    },
    "playwright": {
      "command": "mcp-server-playwright",
      "args": [],
      "env": {
        "BROWSER": "chromium"
      }
    }
  },
  "personality": {
    "traits": [{{#each personality.traits}}"{{this}}"{{#unless @last}}, {{/unless}}{{/each}}],
    "catchphrases": [{{#each personality.catchphrases}}"{{this}}"{{#unless @last}}, {{/unless}}{{/each}}],
    "style": "{{personality.communication_style}}"
  },
  "network": {
    "backend": "http://localhost:{{ports.backend}}",
    "frontend": "http://localhost:{{ports.frontend}}",
    "host": "{{hostName}}"
  },
  "theme": {
    "name": "{{theme}}",
    "emoji": "{{themeEmoji}}"
  },
  "team": {
    "number": {{teamNumber}},
    "role": "{{name}}",
    "updateChannel": "team-{{teamNumber}}-updates",
    "teamChannel": "team-{{teamNumber}}"
  }
}